@using Vandelay.Industries.Models

<ul class="widget-list">
    @foreach (TagCount tagCount in ((IEnumerable<TagCount>)Model.TagCounts).OrderByDescending(x => x.Count).Take(6)) {
        <li>
            @Html.ActionLink(tagCount.TagName, "Search", "Home", new { area = "Orchard.Tags", tagName = tagCount.TagName }, null)
            <span>(@tagCount.Count)</span>
        </li>
    }
</ul>